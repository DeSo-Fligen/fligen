{
  "name": "fligen",
  "version": "0.1.0",
  "private": true,
  "description": "a social application power by blockchain.",
  "main": "public/electron.js",
  "author": {
    "name": "oloshe"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/codemirror": "^5.60.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-syntax-highlighter": "^13.5.2",
    "bootstrap": "5.1.3",
    "codemirror": "^5.63.3",
    "i18next": "^21.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.13.0",
    "react-markdown": "^7.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.4",
    "redux": "^4.1.2",
    "remark-gfm": "^3.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "build-lib": "cargo-cp-artifact --artifact cdylib rlib lib/index.node -- cargo build --target=aarch64-apple-darwin --manifest-path=rlib/Cargo.toml --message-format=json-render-diagnostics",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cargo-cp-artifact": "^0.1.5",
    "concurrently": "^6.3.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.fligen",
    "mac": {
      "category": "DeSo"
    },
    "files": [
      "build/**/*",
      "package.json"
    ]
  }
}
